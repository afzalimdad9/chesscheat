{
  "manifest_version": 3,
  "name": "Chesscheat",
  "version": "1.0",
  "description": "Always see the optimal move in any chess.com game. Guarantees wins almost 100% of the time",
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; script-src-elem 'self' 'unsafe-inline' https://chess.com/;"
},
  "content_scripts": [
    {
      "js": ["scripts/main.js"],
      "matches": [
        "https://www.chess.com/*",
        "https://chess.com/*"
      ]
    }
  ]
}